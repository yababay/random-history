{
	"name": "random-history",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"bro": "BROWSER=firefox vite dev --open",
		"build": "vite build",
		"git": "read -p 'Суть коммита: ' COMMIT; git add . ; git commit -am $COMMIT ; git push origin scratch",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run",
		"redis": "redis-cli -p 6377",
		"restore": "./scripts/restore.sh",
		"psql": "psql -h localhost -U random_history -d random_history",
		"docker:containest": "docker container ls -a",
		"docker:images": "docker image list",
		"docker:bash": "docker exec -it redis-starter /bin/bash",
		"docker:stop": "docker container stop redis-starter",
		"docker:start": "docker run -v /media/portable/.3f-lab/_humanitarian/random-history/.data/:/data --rm -d -p 6377:6379 --env-file .env --name redis-starter redis-starter:random-history",
		"docker:rh": "docker run --rm -d -p 3000:3000 --env-file .env --name redis-starter redis-starter:random-history",
		"docker:build": "npm run build && docker build . -t redis-starter:random-history",
		"docker:publish": "docker tag redis-starter:random-history cr.yandex/crp840llo1tumo6tofpn/redis-starter:random-history && docker push cr.yandex/crp840llo1tumo6tofpn/redis-starter:random-history"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/adapter-node": "^5.3.2",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@types/pg": "^8.15.5",
		"@vitest/browser": "^3.2.3",
		"@yababay67/sveltekit-components": "^2.2.2",
		"playwright": "^1.53.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^7.0.4",
		"vitest": "^3.2.3",
		"vitest-browser-svelte": "^0.1.0"
	},
	"dependencies": {
		"axios": "^1.12.2",
		"dotenv": "^17.2.2",
		"pg": "^8.16.3",
		"redis": "^5.8.2",
		"showdown": "^2.1.0",
		"telegraf": "^4.16.3",
		"vk-io": "^4.9.1"
	}
}
